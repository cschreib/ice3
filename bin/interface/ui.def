<--------------------->
<-- PRE-DEFINITIONS -->
<--------------------->

<d:Region name inherits="" b:virtual="false" b:setAllPoints="false" b:hidden="false">

    <n[.,1]:Size>
        <n[*]:AbsDimension n:x="0"   n:y="0"/>
        <n[*]:RelDimension n:x="1.0" n:y="1.0"/>
    </Size>

    <n[.,1]:Anchors>
        <Anchor point="TOPLEFT" relativeTo="" relativePoint="">
            <n[.,1]:Offset>
                <n[*]:AbsDimension n:x="0"   n:y="0"/>
                <n[*]:RelDimension n:x="0.0" n:y="0.0"/>
            </Offset>
        </Anchor>
    </Anchors>

</Region>

<d[Region]:FontString n:fontHeight="0" text="" n:spacing="0.0" outline="NONE" b:monochrome="false" b:nonspacewrap="false" justifyV="MIDDLE" justifyH="CENTER" n:maxLines="0" font="" n:bytes="255">

    <n[.,1]:Color n:a="1.0" n:r="0.0" n:g="0.0" n:b="0.0"/>

    <n[.,1]:Shadow>
        <n[.,1]:Color n:a="1.0" n:r="0.0" n:g="0.0" n:b="0.0" c=""/>
        <n[.,1]:Offset n:x="0" n:y="0"/>
    </Shadow>

</FontString>

<d[Region]:Texture file="" filter="NONE" alphaMode="BLEND">

    <n[.,1]:TexCoords n:left="0.0" n:right="1.0" n:top="0.0" n:bottom="1.0"/>

    <n[.,1]:Color n:a="1.0" n:r="0.0" n:g="0.0" n:b="0.0" c=""/>

    <n[.,1]:Gradient orientation="HORIZONTAL">
        <n[1,1]:MinColor n:a="1.0" n:r="0.0" n:g="0.0" n:b="0.0" c=""/>
        <n[1,1]:MaxColor n:a="1.0" n:r="0.0" n:g="0.0" n:b="0.0" c=""/>
    </Gradient>

</Texture>

<d[Region]:Frame n:alpha="1.0" parent="" b:topLevel="false" b:movable="false" b:resizable="false" frameStrata="PARENT" b:enableMouse="false" b:enableMouseWheel="false" b:enableKeyboard="false" b:clampedToScreen="false">

    <n[.,1]:ResizeBounds>
        <n[.,1]:Min>
            <n[*]:AbsDimension n:x="0"   n:y="0"/>
            <n[*]:RelDimension n:x="1.0" n:y="1.0"/>
        </Min>
        <n[.,1]:Max>
            <n[*]:AbsDimension n:x="-1"   n:y="-1"/>
            <n[*]:RelDimension n:x="1.0" n:y="1.0"/>
        </Max>
    </ResizeBounds>

    <n[.,1]:c[Region]:TitleRegion name=""/>

    <n[.,1]:Backdrop bgFile="" edgeFile="" b:tile="false">
        <n[.,1]:BackgroundInsets>
            <n[*]:AbsInset n:left="0"   n:right="0"   n:top="0"   n:bottom="0"/>
            <n[*]:RelInset n:left="0.0" n:right="0.0" n:top="0.0" n:bottom="0.0"/>
        </BackgroundInsets>

        <n[.,1]:BackgroundColor n:a="1.0" n:r="0.0" n:g="0.0" n:b="0.0" c=""/>

        <n[.,1]:TileSize>
            <n[*]:AbsValue n:x/>
            <n[*]:RelValue n:x/>
        </TileSize>

        <n[.,1]:EdgeSize>
            <n[*]:AbsValue n:x/>
            <n[*]:RelValue n:x/>
        </EdgeSize>
    </Backdrop>

    <n[.,1]:HitRectInsets>
        <n[*]:AbsInset n:left="0"   n:right="0"   n:top="0"   n:bottom="0"/>
        <n[*]:RelInset n:left="0.0" n:right="0.0" n:top="0.0" n:bottom="0.0"/>
    </HitRectInsets>

    <n[.,1]:Layers>
        <Layer level="ARTWORK">
            <l:Texture/>
            <l:FontString/>
        </Layer>
    </Layers>

    <n[.,1]:Frames>
        <l:Frame/>
    </Frames>

    <n[.,1]:Scripts>
        <n[.,1]:OnDragStart/>
        <n[.,1]:OnDragStop/>
        <n[.,1]:OnEnter/>
        <n[.,1]:OnEvent/>
        <n[.,1]:OnHide/>
        <n[.,1]:OnKeyDown/>
        <n[.,1]:OnKeyUp/>
        <n[.,1]:OnLeave/>
        <n[.,1]:OnLoad/>
        <n[.,1]:OnMouseDown/>
        <n[.,1]:OnMouseUp/>
        <n[.,1]:OnMouseWheel/>
        <n[.,1]:OnReceiveDrag/>
        <n[.,1]:OnShow/>
        <n[.,1]:OnSizeChanged/>
        <n[.,1]:OnUpdate/>
    </Scripts>

</Frame>

<d[Frame]:Button text="" b:enableMouse="true">

    <n[.,1]:c[Texture]:NormalTexture    name="$parentNormalTexture"    b:setAllPoints="true" layer="ARTWORK"/>
    <n[.,1]:c[Texture]:PushedTexture    name="$parentPushedTexture"    b:setAllPoints="true" hidden="true" layer="BORDER"/>
    <n[.,1]:c[Texture]:DisabledTexture  name="$parentDisabledTexture"  b:setAllPoints="true" hidden="true" layer="BORDER"/>
    <n[.,1]:c[Texture]:HighlightTexture name="$parentHighlightTexture" b:setAllPoints="true" hidden="true" layer="HIGHLIGHT"/>

    <n[.,1]:c[FontString]:NormalText    name="$parentNormalText"    b:setAllPoints="true" layer="ARTWORK"/>
    <n[.,1]:c[FontString]:HighlightText name="$parentHighlightText" b:setAllPoints="true" hidden="true" layer="HIGHLIGHT"/>
    <n[.,1]:c[FontString]:DisabledText  name="$parentDisabledText"  b:setAllPoints="true" hidden="true" layer="BORDER"/>

    <n[.,1]:PushedTextOffset>
        <n[*]:AbsDimension n:x="0" n:y="0"/>
        <n[*]:RelDimension n:x="0.0" n:y="0.0"/>
    </PushedTextOffset>

    <n[.,1]:Scripts>
        <n[.,1]:OnClick/>
        <n[.,1]:OnDoubleClick/>
        <n[.,1]:OnDisable/>
        <n[.,1]:OnDragStart/>
        <n[.,1]:OnDragStop/>
        <n[.,1]:OnEnable/>
        <n[.,1]:OnEnter/>
        <n[.,1]:OnEvent/>
        <n[.,1]:OnHide/>
        <n[.,1]:OnKeyDown/>
        <n[.,1]:OnKeyUp/>
        <n[.,1]:OnLeave/>
        <n[.,1]:OnLoad/>
        <n[.,1]:OnMouseDown/>
        <n[.,1]:OnMouseUp/>
        <n[.,1]:OnMouseWheel/>
        <n[.,1]:OnReceiveDrag/>
        <n[.,1]:OnShow/>
        <n[.,1]:OnSizeChanged/>
        <n[.,1]:OnUpdate/>
    </Scripts>

</Button>

<d[Button]:CheckButton b:checked="false">

    <n[.,1]:c[Texture]:CheckedTexture         name="$parentCheckedTexture"         b:setAllPoints="true" b:hidden="true" layer="ARTWORK"/>
    <n[.,1]:c[Texture]:DisabledCheckedTexture name="$parentDisabledCheckedTexture" b:setAllPoints="true" b:hidden="true" layer="ARTWORK"/>

</CheckButton>

<d[Frame]:ColorSelect>

    <n[.,1]:c[Texture]:ColorWheelTexture/>
    <n[.,1]:c[Texture]:ColorWheelThumbTexture/>
    <n[.,1]:c[Texture]:ColorValueTexture/>
    <n[.,1]:c[Texture]:ColorValueThumbTexture/>

    <n[.,1]:Scripts>
        <n[.,1]:OnColorSelect/>
        <n[.,1]:OnDragStart/>
        <n[.,1]:OnDragStop/>
