        <n[.,1]:OnUpdate/>
    </Scripts>

</EditBox>

<d[Frame]:MessageFrame n:fadeDuration="3.0" insertMode>

    <n[.,1]:c[FontString]:FontString name="$parentFontString"/>

    <n[.,1]:TextInsets>
        <n[*]:AbsInset n:left="0"   n:right="0"   n:top="0"   n:bottom="0"/>
        <n[*]:RelInset n:left="0.0" n:right="0.0" n:top="0.0" n:bottom="0.0"/>
    </TextInsets>

</MessageFrame>

