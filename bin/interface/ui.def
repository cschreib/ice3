        <n[.,1]:OnUpdate/>
    </Scripts>

</ColorSelect>

<d[Frame]:Cooldown/>

<d[Frame]:EditBox b:enableMouse="true" b:enableKeyboard="true" n:letters="0" n:blinkSpeed="0.5" b:numeric="false" b:password="false" b:multiLine="false" n:historyLines="0" b:autoFocus="false" b:ignoreArrows="false" b:positive="false" b:integer="false">

    <n[.,1]:c[FontString]:FontString name="$parentFontString"/>

    <n[.,1]:HighlightColor n:a="0.5" n:r="1" n:g="1" n:b="1" c=""/>

    <n[.,1]:TextInsets n:left="0" n:right="0" n:top="0" n:bottom="0"/>

    <n[.,1]:Scripts>
        <n[.,1]:OnChar/>
        <n[.,1]:OnCursorChanged/>
